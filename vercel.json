{
    "buildCommand": "cd dashboard && rm -rf node_modules build && npm install && npm run build",
    "outputDirectory": "dashboard/build",
    "devCommand": "cd dashboard && npm start",
    "installCommand": "echo 'Skipping root install'",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://api.emberdrago.com/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}