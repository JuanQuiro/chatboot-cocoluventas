{
    "buildCommand": "npm run build",
    "outputDirectory": "dashboard/build",
    "devCommand": "cd dashboard && npm start",
    "installCommand": "npm install && npm run dashboard:install",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://api.emberdrago.com/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}