{
  "name": "cocolu-dashoffice",
  "version": "5.0.0",
  "description": "DashOffice - Sistema Central Empresarial con Multi-Bot WhatsApp - Ember Drago",
  "main": "app-integrated.js",
  "scripts": {
    "start": "node iniciar-bot.js",
    "start:phone": "USE_PAIRING_CODE=true node app-integrated.js",
    "start:qr": "USE_PAIRING_CODE=false node app-integrated.js",
    "dev": "node iniciar-bot.js",
    "debug": "node --inspect app-integrated.js",
    "prod": "NODE_ENV=production node app-integrated.js",
    "prod:pm2": "pm2 start ecosystem.config.js --env production",
    "prod:pm2:stop": "pm2 stop cocolu-dashoffice",
    "prod:pm2:restart": "pm2 restart cocolu-dashoffice",
    "prod:pm2:logs": "pm2 logs cocolu-dashoffice",
    "prod:pm2:monit": "pm2 monit",
    "docker:build": "docker build -f Dockerfile.optimized -t cocolu-dashoffice:latest .",
    "docker:run": "docker run -p 3009:3009 --env-file .env cocolu-dashoffice:latest",
    "docker:build:run": "npm run docker:build && npm run docker:run",
    "dashboard": "cd dashboard && npm start",
    "dashboard:build": "cd dashboard && GENERATE_SOURCEMAP=false npm run build",
    "dashboard:build:analyze": "cd dashboard && npm run build && source-map-explorer 'build/static/js/*.js'",
    "dashboard:install": "cd dashboard && npm install",
    "install:all": "npm install && npm run dashboard:install",
    "build:all": "npm run dashboard:build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint \"src/**/*.js\"",
    "format": "prettier --write \"src/**/*.js\"",
    "legacy:start": "node app.js",
    "legacy:dev": "node --watch app.js"
  },
  "keywords": [
    "whatsapp",
    "chatbot",
    "builderbot",
    "cocolu"
  ],
  "author": "Cocolu Ventas",
  "license": "MIT",
  "dependencies": {
    "@builderbot/bot": "^1.1.94",
    "@builderbot/database-json": "^1.1.94",
    "@builderbot/provider-baileys": "^1.1.94",
    "@hapi/boom": "^10.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/cqrs": "^10.2.6",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/platform-express": "^10.3.0",
    "@whiskeysockets/baileys": "^6.7.21",
    "@wppconnect-team/wppconnect": "^1.37.6",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "inquirer": "^8.2.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "puppeteer": "^24.29.1",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.5",
    "tesseract.js": "^6.0.1",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "optionalDependencies": {
    "compression": "^1.7.4",
    "ioredis": "^5.3.2"
  }
}
